;; gen pattern string to use patterns as colors
(defn pattern
  [ fill-id]
  (str "url(#" fill-id ") #fff"))

;; pattern gen fn for defs on init
(defn pattern-def
  [{:keys [id image-link]}]
  [:pattern {:id id
             :patternUnits "userSpaceOnUse"
             :width "10"
             :height "10"
             :key (random-uuid)}
    [:image {:xlinkHref image-link
             :x "0"
             :y "0"
             :width "10"
             :height "10"
             :key (random-uuid)}]])

